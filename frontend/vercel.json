{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/user-management-frontend",
  "framework": "angular",
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*\\.js)", "dest": "/$1" },
    { "src": "/(.*\\.css)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}